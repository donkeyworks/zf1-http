{
    "name": "donkeyworks/zendframework1-http",
    "description": "Zend Framework 1 Http component",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "donkeyworks/zendframework1-exception": "^1.0.0",
        "donkeyworks/zendframework1-config": "^1.0.0",
        "donkeyworks/zendframework1-loader": "^1.0.0",
        "donkeyworks/zendframework1-date": "^1.0.0",
        "donkeyworks/zendframework1-session": "^1.0.0",
        "donkeyworks/zendframework1-uri": "^1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Zend_Http_": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Zend_": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "phpstan/phpstan": "^0.9.2 | ^0.10.0",
        "friendsofphp/php-cs-fixer": "^2.11",
        "maglnet/composer-require-checker": "^0.1.6 | ^1.1.0",
        "phpro/grumphp": "^0.14.0"
    },
    "include-path": [
        "src/"
    ],
    "archive": {
        "exclude": ["/tests"]
    },
    "scripts": {
        "test": "phpunit ./tests",
        "test-with-coverage": "phpunit --coverage-clover=clover.xml ./tests",
        "phpstan": "phpstan analyze -c phpstan.neon --no-progress --ansi",
        "style-check": "php-cs-fixer fix --dry-run -vv"
    },
    "suggest": {
        "ext-curl": "Required by the Zend_Http_Client_Adapter_Curl adapter",
        "ext-zlib": "Required to compress/decompress gzipped request/response",
        "ext-fileinfo": "If installed, will be used for mime type detection"
    }
}
